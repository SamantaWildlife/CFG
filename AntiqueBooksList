# Question 3 books 1800 and 1950

book_year = int(input('What year is your book? '))

def century():
    if book_year >= 1800 and book_year <= 1900:
        century = 'Eighteenth Century'

    elif book_year >= 1900 and book_year <= 2000:
        century = 'Nineteenth Century'

    else:
        print('This book is not antique')

    return century

book_century = century()

decades = ['noughties', 'teens', 'twenties', 'thirties', 'forties' 'fifties']

def book_decade():

    if book_year[-2] == '0':
        return decades

    elif book_year[-2] == '1':
        return decades

    else:
        print('Error')

print('Your book is from the {} '.format(book_century) + book_decade)
